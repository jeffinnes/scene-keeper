    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.17",
    "babel-jest": "^30.2.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "jiti": "^2.5.1",
    "prettier": "3.6.2",
    "ts-node": "^10.9.2",
